{
  "openapi": "3.0.0",
  "info": {
    "title": "CompassCar",
    "version": "1.0.0",
    "description": "This API allows you to manage a rental company's vehicle registration.\nThrough the POST, PATCH, GET and DELETE routes, it is possible to add, update, query and remove vehicles from the system,\nfacilitating the management of the inventory of cars available for rental."
  },
  "servers": [
    {
      "url": "http://localhost:3000/",
      "description": "API de test"
    }
  ],
  "paths": {
    "  /": {
      "post": {
        "tags": [
          "Users"
        ],
        "description": "Register user in the database, where it is necessary to inform the Name, Email  and Paswword.",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/user"
              },
              "example": {
                "name": "Maria Silva",
                "email": "mariasilva@seuprovedor.com",
                "password": "125fadR125We#@"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Created",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "id": {
                      "type": "string uuid",
                      "example": "5bdf4c60-fde2-477f-a054-f80bdcd51e84"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Bad Request"
          },
          "409": {
            "description": "email address already used"
          },
          "500": {
            "description": "internal server error"
          }
        }
      },
      "get": {
        "tags": [
          "Users"
        ],
        "description": "Register user in the database, where it is necessary to inform the Name, Email  and Paswword.",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/user"
              },
              "example": {
                "name": "Maria Silva",
                "email": "mariasilva@seuprovedor.com",
                "password": "125fadR125We#@"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Created",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "id": {
                      "type": "string uuid",
                      "example": "adsgaghafbfsd"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Bad Request"
          },
          "409": {
            "description": "email address already used"
          },
          "500": {
            "description": "internal server error"
          }
        }
      }
    },
    "components": {
      "schemas": {
        "user": {
          "type": "object""properties": "name": "type": "string""required": true
        "email": "type": "string""required": true
        "password": "type": "sring""required": true
        }
      },
      "car": {
        "type": "object",
        "properties": {
          "brand": {
            "type": "string",
            "required": true
          },
          "model": {
            "type": "string",
            "required": true
          },
          "year": {
            "type": "number",
            "required": true
          },
          "items": {
            "type": "array",
            "required": true,
            "items": {
              "type": "string"
            }
          }
        }
      }
    }
  }
}
